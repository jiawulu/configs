# Available Value:
# True False
FuzzyUanUang=True
# Fuzzy c and ch
# Available Value:
# True False
FuzzyCCh=True
# Fuzzy f and h
# Available Value:
# True False
FuzzyFH=False
# Fuzzy l and n
# Available Value:
# True False
FuzzyLN=False
# Fuzzy s and sh
# Available Value:
# True False
FuzzySSH=True
# Fuzzy z and zh
